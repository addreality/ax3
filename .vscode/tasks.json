{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "hxml",
			"file": "build.hxml",
			"identifier": "build",
			"problemMatcher": [
				"$haxe-absolute",
				"$haxe",
				"$haxe-error",
				"$haxe-trace"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"echo": true,
				"reveal": "silent",
				"focus": false,
				"panel": "shared"
			}
		}
	]
}
